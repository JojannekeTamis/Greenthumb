<h2>Edit your garden</h2>
<%= simple_form_for(@garden, url: owner_garden_path) do |f| %>
  <%= f.input :address %>
  <%= f.input :description %>
  <%= f.input :available %>
  <%= f.input :photo %>
  <%= f.input :photo_cache, as: :hidden %>
  <%= f.input :photo2 %>
  <%= f.input :photo2_cache, as: :hidden %>
  <%= f.input :photo3 %>
  <%= f.input :photo3_cache, as: :hidden %>
  <%= f.submit %>
<% end %>
